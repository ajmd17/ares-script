/* Standard definitions */
alias str = Convert.toString;
alias string = Convert.toString;
alias int = Convert.toInt;
alias float = Convert.toFloat;
alias bool = Convert.toBool;
alias system = System.process;
alias input = Console.readln;
alias fopen = FileIO.open;
alias fwrite = FileIO.write;
alias fread = FileIO.read;
alias fclose = FileIO.close;